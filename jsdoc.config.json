{
  "plugins": ["jsdoc/eslint-config-jsdoc"],
  "source": {
    "include": ["./src"],
    "includePattern": "\\.(js|jsx)$",
    "exclude": ["node_modules/", "tests/"]
  },
  "opts": {
    "destination": "./docs/generated/",
    "recurse": true
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}